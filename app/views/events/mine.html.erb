<div class="container">
  <h2>My Events</h2>


</div>
<div class="container">
  <h2>Basic Table</h2>
  <p>The .table class adds basic styling (light padding and only horizontal dividers) to a table:</p>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Creator</th>
        <th>Begins</th>
        <th>Ends</th>
      </tr>
    </thead>
    <tbody>
      <% @events.each do |e| %>
      <tr>
        <td><%= e.name %></td>
        <td><%= e.user_id %></td>
        <td><%= e.starts_at.to_formatted_s(:long_ordinal) %></td>
        <td><%= e.ends_at.to_formatted_s(:long_ordinal) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
