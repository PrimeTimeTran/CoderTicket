<br><br>
<div class="container">
  <h1>New Event</h1>
    <%= form_for [@event] do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control"%><br>

    <%= f.label :venue_id %>
    <%= f.text_field :venue_id, class: "form-control"%><br>

    <%= f.label :starts_at %>
    <%= f.text_field :starts_at, class: "form-control"%><br>

    <%= f.label :ends_at %>
    <%= f.text_field :ends_at, class: "form-control"%><br>

    <%= f.label :extended_html_description %>
    <%= f.text_field :extended_html_description, class: "form-control"%><br>

    <%= f.label :category %>
    <%= f.text_field :category, class: "form-control"%><br>

    <%= f.submit :Submit, class: "btn btn-success" %>
  <% end %>

</div>
